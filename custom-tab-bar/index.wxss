.tab-bar, .on-tabbar{
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  display: flex;
  padding-bottom: env(safe-area-inset-bottom);
}

.tab-bar{
  background: rgba(23, 129, 181, .2);
  height: 96rpx;
}

.on-tabbar{
  width: 100%;
  z-index: 1002;
  height: 125rpx;
  pointer-events: none;
  justify-content: center;
  align-items: center;
}

.tab-bar-border {
  background-color: rgba(0, 0, 0, 0.33);
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 1px;
  transform: scaleY(0.5);
}

.tab-bar-item {
  flex: 1;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  z-index: 500;
}

.tab-bar-item image {
  width: 54rpx;
  height: 54rpx;
}

.tab-bar-item view {
  font-size: 10px;
}

.menu{
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 50%;
  top:-266.5rpx;
  /* background-color: cyan; */
  width: 600rpx;
  height: 600rpx;
  position: relative;
  z-index: 1000;
}

.menuitem, .menutext{
  width: 125rpx;
  height: 125rpx;
  border-radius: 50%;
  pointer-events: auto;
  text-align: center;
  line-height: 125rpx;
  background-size:contain;
  z-index: 1001;
}

.menutext{
  background-color: #1781b5;
  box-shadow: 0 0 30rpx 6rpx rgba(23, 129, 181, 0.8);
}

.menuitem{
  position: absolute;
  background-repeat: no-repeat;
  /* background-color:rgba(125, 200, 125, 1); */
  background-color:rgba(23, 129, 181, 0.4);
}

.tabbarmenu, .inner{
  display: flex;
  justify-content: center;
}
.tabbarmenu{
  width: 250rpx;
}


